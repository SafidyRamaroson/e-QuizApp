{
  "name": "src",
  "version": "1.0.0",
  "description": "Quiz App about JavaScript and React JS",
  "main": "src/index.ts",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon --watch src/**/*.ts --exec npx tsx src/index.ts",
    "serve": "node dist/src/index.js",
    "build": "tsc",
    "prisma:format": "prisma format",
    "prisma:migrate": "prisma migrate dev --skip-seed --preview-feature",
    "prisma:generate": "prisma generate",
    "prisma:generate:watch": "prisma generate --watch",
    "prisma:seed": "ts-node prisma/seed/seed.ts --preview-feature",
    "prisma:reset": "prisma migrate reset",
    "prisma:reset:force": "prisma migrate reset -f"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.12",
    "@types/nodemailer": "^6.4.15",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.1",
    "prisma": "^5.14.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@prisma/client": "^5.14.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
